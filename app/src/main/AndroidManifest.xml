<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shashank.dbzhangmanmultiplayer" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity1"
            android:label="@string/title_activity_1"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".Activity2"
            android:label="@string/title_activity_2"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Activity3"
            android:label="@string/title_activity_3"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Activity4"
            android:label="@string/title_activity_4"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity5"
            android:label="@string/title_activity_5"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
